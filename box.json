{
    "alias": "php_backslasher.phar",
    "chmod": "0755",
    "directories": [
        "src"
    ],
    "finder": [
        {
            "name": "*.php",
            "in": "vendor",
            "exclude": [
                "doctrine",
                "fabpot",
                "justinrainbow",
                "phpdocumentor",
                "phpseclib",
                "phpspec",
                "phpunit",
                "sebastian",
                "seld",
                "tedivm",
                "twig",
                "tests"
            ]
        }
    ],
    "git-version": "package_version",
    "main": "src/bootstrap.php",
    "output": "build/php_backslasher.phar",
    "stub": true
}